<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="de.hswt.hrm.main"
            application="org.eclipse.e4.ui.workbench.swt.E4Application">
         <property
               name="clearPersistedState"
               value="true">
         </property>
         <property
               name="applicationCSS"
               value="platform:/plugin/de.hswt.hrm.main/css/default.css">
         </property>
         <property
               name="appName"
               value="de.hswt.hrm.main">
         </property>
          <property
           name="lifeCycleURI"
           value="bundleclass://de.hswt.hrm.main/de.hswt.hrm.main.handlers.LifeCycleHandler">
         </property>
          <property
                name="preferenceCustomization"
                value="plugin_customization.ini">
          </property>
          <property
                name="windowImages"
                value="icons/16_32.png,icons/32_32.png,icons/48_32.png,icons/64_32.png,icons/128_32.png">
          </property>
      </product>
   </extension>
</plugin>
