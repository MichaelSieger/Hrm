<Composite xmlns="http://www.eclipse.org/xwt/presentation"
	xmlns:x="http://www.eclipse.org/xwt" xmlns:p1="clr-namespace:de.hswt.hrm.place.ui.xwt"
	x:Class="de.hswt.hrm.catalog.ui.event.CatalogMatchingEventHandler"
	xmlns:p2="clr-namespace:org.eclipse.ui.forms.widgets">
	<Composite.layout>
		<GridLayout numColumns="4" />
	</Composite.layout>
	<Button text="Back" x:Name="back2Main" />
	<Label />
	<Label />
	<Label />
	<Section text="Kataloge" x:Style="(p2:Section).EXPANDED | (p2:Section).TITLE_BAR"
		x:Name="" titleBarBackground="Darkorange" titleBarBorderColor="Darkorange" titleBarForeground="ForestGreen">
		<Section.layoutData>
			<GridData horizontalAlignment="FILL" verticalAlignment="FILL"
				heightHint="177" grabExcessVerticalSpace="true"
				grabExcessHorizontalSpace="true" widthHint="202" />
		</Section.layoutData>
		<Section.client>
			<Composite>
				<Composite.layout>
					<GridLayout />
				</Composite.layout>
				<ListViewer x:Style="BORDER | V_SCROLL" x:Name="catalogs">
					<ListViewer.list x:Name="" SelectionEvent="onSelection">
						<ListViewer.list.layoutData>
							<GridData horizontalAlignment="FILL" verticalAlignment="FILL"
								grabExcessHorizontalSpace="true" grabExcessVerticalSpace="true"
								widthHint="281" />
						</ListViewer.list.layoutData>
					</ListViewer.list>
				</ListViewer>
			</Composite>
		</Section.client>
	</Section>
	<Section text="Zugeordnete Soll Zustnde" x:Style="(p2:Section).EXPANDED | (p2:Section).TITLE_BAR">
		<Section.layoutData>
			<GridData grabExcessVerticalSpace="true"
				horizontalAlignment="FILL" verticalAlignment="FILL" widthHint="300"
				grabExcessHorizontalSpace="true" />
		</Section.layoutData>
		<Section.client>
			<Composite>
				<Composite.layout>
					<GridLayout />
				</Composite.layout>
				<ListViewer x:Style="BORDER | V_SCROLL" x:Name="matchedTarget">
					<ListViewer.list x:Name="">
						<ListViewer.list.layoutData>
							<GridData horizontalAlignment="FILL" verticalAlignment="FILL"
								grabExcessHorizontalSpace="true" grabExcessVerticalSpace="true" />
						</ListViewer.list.layoutData>
					</ListViewer.list>
				</ListViewer>
			</Composite>
		</Section.client>
	</Section>
	<Section text="Zugeordnete Ist" x:Style="(p2:Section).EXPANDED | (p2:Section).TITLE_BAR">
		<Section.layoutData>
			<GridData grabExcessVerticalSpace="true" verticalAlignment="FILL"
				horizontalAlignment="FILL" widthHint="300"
				grabExcessHorizontalSpace="true" />
		</Section.layoutData>
		<Section.client>
			<Composite>
				<Composite.layout>
					<GridLayout />
				</Composite.layout>
				<ListViewer x:Style="BORDER | V_SCROLL" x:Name="matchedCurrent">
					<ListViewer.list x:Name="">
						<ListViewer.list.layoutData>
							<GridData horizontalAlignment="FILL" verticalAlignment="FILL"
								grabExcessHorizontalSpace="true" grabExcessVerticalSpace="true" />
						</ListViewer.list.layoutData>
					</ListViewer.list>
				</ListViewer>
			</Composite>
		</Section.client>
	</Section>
	<Section text="Zugeordnete Manahmen" x:Style="(p2:Section).EXPANDED | (p2:Section).TITLE_BAR">
		<Section.layoutData>
			<GridData grabExcessVerticalSpace="true"
				horizontalAlignment="FILL" verticalAlignment="FILL"
				grabExcessHorizontalSpace="true" widthHint="300" />
		</Section.layoutData>
		<Section.client>
			<Composite>
				<Composite.layout>
					<GridLayout />
				</Composite.layout>
				<ListViewer x:Style="BORDER | V_SCROLL" x:Name="matchedActivity">
					<ListViewer.list x:Name="">
						<ListViewer.list.layoutData>
							<GridData grabExcessHorizontalSpace="true"
								grabExcessVerticalSpace="true" horizontalAlignment="FILL"
								verticalAlignment="FILL" />
						</ListViewer.list.layoutData>
					</ListViewer.list>
				</ListViewer>
			</Composite>
		</Section.client>
	</Section>
	<Section text="Informationen" x:Style="(p1:Section).EXPANDED | (p1:Section).TITLE_BAR"
		x:Name="information">
		<Section.layoutData>
			<GridData verticalAlignment="FILL" horizontalAlignment="FILL"
				grabExcessHorizontalSpace="true" widthHint="306" />
		</Section.layoutData>
		<Section.client>
			<Composite>
				<Composite.layout>
					<GridLayout numColumns="4" />
				</Composite.layout>
				<Label />
				<Label />
				<Label />
				<Label />
				<Label text="Name :" />
				<Label />
				<Label />
				<Text x:Style="BORDER" x:Name="name">
					<Text.layoutData>
						<GridData grabExcessHorizontalSpace="true"
							horizontalAlignment="FILL" verticalAlignment="FILL" />
					</Text.layoutData>
				</Text>
				<Label text="Beschreibung :" />
				<Text x:Style="BORDER | WRAP | V_SCROLL | MULTI" x:Name="desc">
					<Text.layoutData>
						<GridData grabExcessHorizontalSpace="true"
							horizontalAlignment="FILL" verticalAlignment="FILL" heightHint="338"
							widthHint="217" horizontalSpan="3" />
					</Text.layoutData>
				</Text>
			</Composite>
		</Section.client>
	</Section>
	<Section text="Verfügbare &#10;Soll " x:Style="(p2:Section).EXPANDED | (p2:Section).TITLE_BAR"
		x:Name="">
		<Section.layoutData>
			<GridData horizontalAlignment="FILL"
				verticalAlignment="FILL" grabExcessVerticalSpace="true"
				grabExcessHorizontalSpace="true" />
		</Section.layoutData>
		<Section.client>
			<Composite>
				<Composite.layout>
					<GridLayout />
				</Composite.layout>
				<Composite>
					<Composite.layout>
						<GridLayout numColumns="3" />
					</Composite.layout>
					<Composite.layoutData>
						<GridData heightHint="36" />
					</Composite.layoutData>
					<Text x:Style="BORDER | H_SCROLL | SEARCH | CANCEL" bounds="0, 0, 75, 27">
						<Text.layoutData>
							<GridData horizontalAlignment="FILL" />
						</Text.layoutData>
					</Text>
					<Button bounds="0, 0, 83, 27" x:Style="ARROW | UP" x:Name="targetUp" SelectionEvent="onMouseDoubleClick" />
					<Button text="New Button" bounds="0, 0, 83, 27" x:Style="ARROW | DOWN" />
				</Composite>
				<ListViewer x:Style="BORDER | V_SCROLL" x:Name="availableTarget">
					<ListViewer.list x:Name="" MouseDoubleClickEvent="onMouseDoubleClick">
						<ListViewer.list.layoutData>
							<GridData horizontalAlignment="FILL" verticalAlignment="FILL"
								grabExcessHorizontalSpace="true" grabExcessVerticalSpace="true" />
						</ListViewer.list.layoutData>
					</ListViewer.list>
				</ListViewer>
			</Composite>
		</Section.client>
	</Section>
	<Section text="Verfügbare &#10;Ist" x:Style="(p2:Section).EXPANDED | (p2:Section).TITLE_BAR"
		x:Name="">
		<Section.layoutData>
			<GridData verticalAlignment="FILL" horizontalAlignment="FILL"
				grabExcessVerticalSpace="true" grabExcessHorizontalSpace="true" />
		</Section.layoutData>
		<Section.client>
			<Composite>
				<Composite.layout>
					<GridLayout />
				</Composite.layout>
				<Composite>
					<Composite.layout>
						<GridLayout numColumns="3" />
					</Composite.layout>
					<Text x:Style="BORDER">
						<Text.layoutData>
							<GridData horizontalAlignment="FILL" />
						</Text.layoutData>
					</Text>
					<Button text="New Button" x:Style="ARROW | UP" />
					<Button text="New Button" x:Style="ARROW | DOWN" />
				</Composite>
				<ListViewer x:Style="BORDER | V_SCROLL" x:Name="availableCurrent">
					<ListViewer.list x:Name="">
						<ListViewer.list.layoutData>
							<GridData grabExcessHorizontalSpace="true"
								grabExcessVerticalSpace="true" horizontalAlignment="FILL"
								verticalAlignment="FILL" />
						</ListViewer.list.layoutData>
					</ListViewer.list>
				</ListViewer>
			</Composite>
		</Section.client>
	</Section>
	<Section text="Verfügbare Maßnahmen" x:Style="(p2:Section).EXPANDED | (p2:Section).TITLE_BAR"
		x:Name="">
		<Section.layoutData>
			<GridData horizontalAlignment="FILL" verticalAlignment="FILL"
				grabExcessVerticalSpace="true" grabExcessHorizontalSpace="true" />
		</Section.layoutData>
		<Section.client>
			<Composite>
				<Composite.layout>
					<GridLayout />
				</Composite.layout>
				<Composite>
					<Composite.layout>
						<GridLayout numColumns="3" />
					</Composite.layout>
					<Text x:Style="BORDER" bounds="0, 0, 75, 27">
						<Text.layoutData>
							<GridData horizontalAlignment="FILL" />
						</Text.layoutData>
					</Text>
					<Button text="New Button" bounds="0, 0, 83, 27" x:Style="ARROW | UP" />
					<Button text="New Button" bounds="0, 0, 83, 27" x:Style="ARROW | DOWN" />
				</Composite>
				<ListViewer x:Style="BORDER | V_SCROLL" x:Name="availableActivity">
					<ListViewer.list x:Name="">
						<ListViewer.list.layoutData>
							<GridData verticalAlignment="FILL" horizontalAlignment="FILL"
								grabExcessHorizontalSpace="true" grabExcessVerticalSpace="true" />
						</ListViewer.list.layoutData>
					</ListViewer.list>
				</ListViewer>
			</Composite>
		</Section.client>
	</Section>
	<Composite xmlns="http://www.eclipse.org/xwt/presentation"
		xmlns:x="http://www.eclipse.org/xwt">
		<Composite.layout>
			<GridLayout />
		</Composite.layout>
	</Composite>
	<Label />
	<Label />
	<Label />
	<!-- Forms API -->
</Composite>