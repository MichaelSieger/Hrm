	<Button image="/resources/icons/Report.jpg" x:Name="toInspection">
		<Button.layoutData>
			<GridData horizontalAlignment="RIGHT" verticalAlignment="CENTER" grabExcessHorizontalSpace="false"/>
		</Button.layoutData>
	</Button>


	<Button x:Name="toCatalog" image="/resources/icons/Catalog.jpg">
