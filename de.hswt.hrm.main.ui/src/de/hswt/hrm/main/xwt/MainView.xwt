	</Label>
	<Label/>
	<Label
		text="Anlagen&#13;&#10;-Anlagen anlegen&#13;&#10;-Anlagen bearbeiten" alignment="RIGHT">
		<Label.layoutData>
			<GridData grabExcessHorizontalSpace="true" horizontalAlignment="FILL"/>
		</Label.layoutData>
	</Label>
	<Button x:Name="toPlants" image="/resources/icons/Plants.jpg">
		<Button.layoutData>
			<GridData horizontalAlignment="RIGHT" verticalAlignment="CENTER" grabExcessHorizontalSpace="false"/>
