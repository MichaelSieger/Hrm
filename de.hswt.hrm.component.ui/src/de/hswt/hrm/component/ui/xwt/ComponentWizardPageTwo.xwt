								<GridLayout makeColumnsEqualWidth="true" numColumns="4" marginHeight="1" marginLeft="5"/>
							</Composite.layout>
							<Label x:Name="imageRL" background="COLOR_WIDGET_BACKGROUND">
								<Label.layoutData>
									<GridData widthHint="100" heightHint="100"/>
								</Label.layoutData>
							</Label>
							<Text x:Style="BORDER" enabled="false" x:Name="pathRL">
								<Text.layoutData>
									<GridData horizontalAlignment="FILL" horizontalSpan="2"/>
								</Text.layoutData>
							</Text>
							<Button text="Upload" x:Name="buttonRL" size="50, 25">
								<Button.layoutData>
									<GridData horizontalAlignment="CENTER"/>
								</Button.layoutData>
							</Button>
						</Composite>
					</Section.client>
				</Section>
				<Section text="pathUD" x:Style="(p2:Section).EXPANDED | (p2:Section).TITLE_BAR">
					<Section.layoutData>
						<GridData grabExcessHorizontalSpace="true" horizontalAlignment="FILL" widthHint="480"/>
